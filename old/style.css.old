@keyframes lpg-text-slide-left {
  0% { padding-left: 80px; padding-right: 0px }
  100% { padding-left: 40px; padding-right: 40px; }
}

@-webkit-keyframes lpg-text-slide-left {
  0% { padding-left: 80px; padding-right: 0px }
  100% { padding-left: 40px; padding-right: 40px; }
}

@keyframes lpg-text-slide-up {
  0% { padding-top: 120px; padding-bottom: 40px }
  100% { padding-top: 80px; padding-bottom: 80px; }
}

@-webkit-keyframes lpg-text-slide-up {
  0% { padding-top: 120px; padding-bottom: 40px }
  100% { padding-top: 80px; padding-bottom: 80px; }
}

@keyframes lpg-text-slide-up-interior {
  0% { padding-top: 100px; padding-bottom: 50px }
  100% { padding-top: 70px; padding-bottom: 80px;  }
}

@-webkit-keyframes lpg-text-slide-up-interior {
  0% { padding-top: 100px; padding-bottom: 50px; }
  100% { padding-top: 70px; padding-bottom: 80px; }
}

@media (min-width: 768px) {
  @keyframes lpg-text-slide-up {
    0% { padding-top: 240px; padding-bottom: 160px }
    100% { padding-top: 200px; padding-bottom: 200px; }
  }

  @-webkit-keyframes lpg-text-slide-up {
    0% { padding-top: 240px; padding-bottom: 160px }
    100% { padding-top: 200px; padding-bottom: 200px; }
  }
}

@media (min-width: 1024px) {
  @keyframes lpg-text-slide-up {
    0% { padding-top: 215px; }
    100% { padding-top: 175px; }
  }

  @-webkit-keyframes lpg-text-slide-up {
    0% { padding-top: 290px; padding-bottom: 210px }
    100% { padding-top: 250px; padding-bottom: 250px; }
  }
}

.lpg-upstep-interior {
  -webkit-animation: lpg-text-slide-up-interior;
  -webkit-animation-iteration-count: 1;
  -webkit-animation-duration: 1s;
  -webkit-animation-timing-function: linear;
  -webkit-animation-direction: normal;   
  animation: lpg-text-slide-up-interior;
  animation-iteration-count: 1;
  animation-duration: 1s;
  animation-timing-function: linear;
  animation-direction: normal;   
}

.lpg-upstep {
  -webkit-animation: lpg-text-slide-up;
  -webkit-animation-iteration-count: 1;
  -webkit-animation-duration: 1s;
  -webkit-animation-timing-function: linear;
  -webkit-animation-direction: normal;   
  animation: lpg-text-slide-up;
  animation-iteration-count: 1;
  animation-duration: 1s;
  animation-timing-function: linear;
  animation-direction: normal;   
}

.lpg-sidestep {
  -webkit-animation: lpg-text-slide-left;
  -webkit-animation-iteration-count: 1;
  -webkit-animation-duration: 1s;
  -webkit-animation-timing-function: linear;
  -webkit-animation-direction: normal;   
  animation: lpg-text-slide-left;
  animation-iteration-count: 1;
  animation-duration: 1s;
  animation-timing-function: linear;
  animation-direction: normal;   
}

@keyframes lpg-title-settle {
  0% { font-size: 34px; letter-spacing: 6.0px; }
  90% { font-size: 27px; letter-spacing: 2.7px; }
  100% { font-size: 26.6px; letter-spacing: 2.66px; }
}

@-webkit-keyframes lpg-title-settle {
  0% { font-size: 34px; letter-spacing: 6.0px; }
  90% { font-size: 27px; letter-spacing: 2.7px; }
  100% { font-size: 26.6px; letter-spacing: 2.66px; }
}

@keyframes lpg-text-white-settle {
  /*0% { color: rgba(255,255,255,0); font-size: 34px; letter-spacing: 6.0px; }
  100% { color: rgba(255,255,255,1); font-size: 26.6px; letter-spacing: 2.66px; } }*/
  0% { color: rgba(255,255,255,0); }
  100% { color: rgba(255,255,255,1); } }

@-webkit-keyframes lpg-text-white-settle {
  /*0% { color: rgba(255,255,255,0); font-size: 34px; letter-spacing: 6.0px; }
  100% { color: rgba(255,255,255,1); font-size: 26.6px; letter-spacing: 2.66px; } }*/
  0% { color: rgba(255,255,255,0); }
  100% { color: rgba(255,255,255,1); } }
}

@keyframes lpg-text-blue-settle {
  /*0% { color: rgba(36,82,160,0); font-size: 34px; letter-spacing: 6.0px; }
  100% { color: rgba(36,82,160,1); font-size: 26.6px; letter-spacing: 2.66px; } }*/
  0% { color: rgba(36,82,160,0); }
  100% { color: rgba(36,82,160,1); } }

@-webkit-keyframes lpg-text-blue-settle {
  /*0% { color: rgba(36,82,160,0); font-size: 34px; letter-spacing: 6.0px; }
  100% { color: rgba(36,82,160,1); font-size: 26.6px; letter-spacing: 2.66px; } }*/
  0% { color: rgba(36,82,160,0); }
  100% { color: rgba(36,82,160,1); } }
}

.lpg-text-blue-effect {
  -webkit-animation: lpg-text-blue-settle;
  -webkit-animation-iteration-count: 1;
  -webkit-animation-duration: 1.5s;
  -webkit-animation-timing-function: linear;
  -webkit-animation-direction: normal;   
  animation: lpg-text-blue-settle;
  animation-iteration-count: 1;
  animation-duration: 1.5s;
  animation-timing-function: linear;
  animation-direction: normal;   
}

.lpg-text-white-effect {
  -webkit-animation: lpg-text-white-settle;
  -webkit-animation-iteration-count: 1;
  -webkit-animation-duration: 1.5s;
  -webkit-animation-timing-function: linear;
  -webkit-animation-direction: normal;   
  animation: lpg-text-white-settle;
  animation-iteration-count: 1;
  animation-duration: 1.5s;
  animation-timing-function: linear;
  animation-direction: normal;   
}

